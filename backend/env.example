# Myntra AI Backend Environment Variables
# Copy this file to .env and fill in your actual API keys

# API Keys (Required)
UNLEASH_NFT_API_KEY=your_unleash_nft_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Flask Configuration (Optional)
SECRET_KEY=your_secret_key_here_or_leave_empty_for_random
FLASK_ENV=production
FLASK_DEBUG=False

# Application Settings (Optional)
LOG_LEVEL=INFO
REQUEST_TIMEOUT=30
API_TIMEOUT=60

# CORS Settings (Optional)
CORS_ORIGINS=chrome-extension://*,moz-extension://*

# Resource Limits (Optional)
MEMORY_LIMIT=512M
CPU_LIMIT=0.5

# Health Check Settings (Optional)
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10 